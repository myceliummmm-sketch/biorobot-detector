# Telegram Bot Token (get from @BotFather)
COMMUNITY_BOT_TOKEN=your_bot_token_here

# Google Gemini API Key (get from https://makersuite.google.com/app/apikey)
GEMINI_API_KEY=your_gemini_api_key_here

# Chat ID where bot should be active (optional, 0 = all chats)
COMMUNITY_CHAT_ID=0

# Response probability (0.0 to 1.0) - how often bot responds randomly
RESPONSE_PROBABILITY=0.3
